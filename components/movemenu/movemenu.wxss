/* components/menu/menu.wxss */
movable-view {
  pointer-events: auto;
}

movable-area {
  pointer-events: none;
}

/* 必须设置movable-area可移动区域 */

.menuarea {
  position: fixed;
  top: 90rpx;
  left: 50%;
  transform: translateX(-50%);
  overflow: hidden;
  z-index: 233;
}

.menu_box,.menu_box2 {
  position: absolute;
  /* top: 0; */
  /* left: 100%; */
  width: 80rpx;
  height: 80rpx;
  color: #fff;
  box-sizing: border-box;
  /* border: 2rpx solid #fff;
  border-radius: 8rpx;
  background-color: #313247; */
  display: flex;
  justify-content: center;
  align-items: center;
}

.menu_box2{
   height: 111rpx;
}

.menu_main_wrap, .menu_main_wrap2 {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border: 2rpx solid #fff;
  border-radius: 8rpx;
  background-color: #313247;
}

.menu_main_wrap2 {
  position: absolute;
}

.menu_main {
  width: 80rpx;
  height: auto;
  padding: 10rpx;
  text-align: center;
}

.menu_img {
  /* width: 50rpx;
  height: 50rpx; */
  vertical-align: middle;
  text-align: center;
}

.menu_title {
  display: block;
  width: 100%;
  text-align: center;
  font-size: 30rpx;
}

